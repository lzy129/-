<view class='find'>
  <input type='text' bindinput='input'></input>
  <button bindtap='find'>搜索</button>
</view>

<view class='bigbox'>
  <view wx:for="{{arr}}" wx:key="index" style='height:15vh;' data-leibie='{{item.leibie}}' data-id='{{item.id}}' bindtap='send'>
    <view style='font-size:36rpx'>{{item.timu}}</view>
    <text style='font-size:28rpx'>{{item.jianjie}}</text>
  </view>
</view>
